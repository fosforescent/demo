  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "start": "vite",
    "test": "npx jest",
    "test:ci": "npx jest --ci",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix ./**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "npx test-storybook",
    "test-test-s": "wait-on tcp:6006 && npx test-storybook\"",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"storybook build --quiet && npx http-server storybook-static --port 6006 --silent\" \"npx wait-on -d 60 -i 1000 tcp:127.0.0.1:6006 && npx test-storybook\""
  },
